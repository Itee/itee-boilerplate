{
  "name": "itee-boilerplate",
  "version": "1.5.9",
  "description": "Itee boilerplate contains all bases packages ( you want ! ) to create a great project.",
  "main": "builds/my_app_name.js",
  "scripts": {
    "postinstall": "node scripts/install-manager.js --mode:install",
    "postuninstall": "node scripts/install-manager.js --mode:uninstall",
    "help": "node npm-scripts-help",
    "doc": "gulp --gulpfile scripts/gulpfile.js doc",
    "test": "npm-run-all --serial --aggregate-output unit bench",
    "unit": "karma start configs/karma.conf.unit.js",
    "bench": "karma start configs/karma.conf.bench.js",
    "mocha": "mocha tests/units/itee-ui.unit.js --reporter mochawesome --reporter-options reportDir=./tests/units/reports/,reportFilename=itee-ui.report",
    "lint": "standard --verbose --pedantic --fix | snazzy",
    "build": "gulp --gulpfile scripts/gulpfile.js build",
    "release": "npm-run-all --parallel --aggregate-output lint doc test --serial --aggregate-output build"
  },
  "keywords": [
    "itee",
    "boilerplate",
    "project",
    "starter"
  ],
  "author": {
    "name": "Tristan Valcke",
    "url": "https://github.com/Itee"
  },
  "contributors": [
    {}
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Itee/itee-boilerplate"
  },
  "devDependencies": {},
  "dependencies": {
    "fs-extra": "^4.0.2"
  },
  "standard": {
    "ignore": [
      "builds"
    ],
    "parser": "babel-eslint"
  }
}
