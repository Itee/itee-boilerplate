{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": ["README.md", "LICENSE.md", "sources/"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(node_modules|documentation|builds)"
  },
  "sourceType": "module",
  "plugins": [],
  "recurseDepth": 3,
  "opts": {
    "template": "node_modules/docdash/",
    "encoding": "utf8",
    "destination": "./documentation/",
    "recurse": true,
    "verbose": true,
    "private": true,
    "tutorials": "tutorials/"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "includeDate": false
    }
  }
}
